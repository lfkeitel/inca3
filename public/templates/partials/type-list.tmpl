{{define "partial-type-list"}}
<div class="table-responsive">
    <table class="type-list table table-striped table-hover">
        <thead>
            <tr>
                <th>Name</th>
                <th>Username</th>
                <th>Script</th>
            </tr>
        </thead>
        <tbody>
            {{range .}}
            <tr class="clickable-row" data-href="profiles/{{.Slug}}">
                <td>
                    {{.Name}}
                </td>
                <td>
                    {{.Username}}
                </td>
                <td>
                    {{.Script}}
                </td>
            </tr>
            {{end}}
        </tbody>
    </table>
</div>
{{end}}